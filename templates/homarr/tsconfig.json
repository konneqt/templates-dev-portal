{
	"extends": "@docusaurus/tsconfig",
	"compilerOptions": {
		"baseUrl": ".",
		"target": "es5",
		"lib": [
			"dom",
			"dom.iterable",
			"esnext"
		],
		"allowJs": true,
		"skipLibCheck": true,
		"strict": true,
		"forceConsistentCasingInFileNames": true,
		"noEmit": true,
		"esModuleInterop": true,
		"module": "esnext",
		"moduleResolution": "node",
		"resolveJsonModule": true,
		"isolatedModules": true,
		"jsx": "preserve",
		"incremental": true
	},
	"include": [
		"**/*.ts",
		"**/*.jsx",
		"**/*.js",
		"**/*.tsx",
		"**/*.md",
		"**/*.mdx"
	],
	"exclude": [
		"node_modules"
	]
}